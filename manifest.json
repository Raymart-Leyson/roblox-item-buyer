{
  "manifest_version": 3,
  "name": "Roblox Buy Link Opener",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "host_permissions": [
    "https://www.roblox.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
        "matches": ["https://www.roblox.com/*"],
        "js": ["content.js"]
    }
  ]  
}
